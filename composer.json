{
    "name": "eonx-com/easy-psr7-factory",
    "description": "Provides an easy way to create PSR7 Request/Response from Symfony Request/Response",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/psr-http-message-bridge": "^1.1",
        "nyholm/psr7": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.4",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.2 || ^5.0",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPsr7Factory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPsr7Factory\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
