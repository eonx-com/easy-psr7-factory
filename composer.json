{
    "name": "loyaltycorp/easy-psr7-factory",
    "description": "Provides an easy way to create PSR7 Request/Response from Symfony Request/Response",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "symfony/psr-http-message-bridge": "^1.1",
        "zendframework/zend-diactoros": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "laravel/lumen-framework": "^5.5",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "LoyaltyCorp\\EasyPsr7Factory\\": "src",
            "LoyaltyCorp\\EasyPsr7Factory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoyaltyCorp\\EasyPsr7Factory\\Tests\\": "tests",
            "LoyaltyCorp\\EasyPsr7Factory\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    }
}
